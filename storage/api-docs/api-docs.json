{
    "openapi": "3.0.0",
    "info": {
        "title": "User Controller",
        "version": "0.1"
    },
    "paths": {
        "/users": {
            "get": {
                "tags": [
                    "Users"
                ],
                "summary": "Get list of users",
                "operationId": "7e2f5799553ec93c9c43e2a58490d447",
                "responses": {
                    "200": {
                        "description": "List of users",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "array",
                                    "items": {
                                        "$ref": "#/components/schemas/User"
                                    }
                                }
                            }
                        }
                    }
                }
            },
            "post": {
                "tags": [
                    "Users"
                ],
                "summary": "Create a new user",
                "operationId": "a1fce870c566eb5fc0cf3178ec5d2fed",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "cpf",
                                    "name",
                                    "email"
                                ],
                                "properties": {
                                    "cpf": {
                                        "type": "integer",
                                        "example": "12345678900"
                                    },
                                    "name": {
                                        "type": "string",
                                        "example": "Itor Carlos"
                                    },
                                    "email": {
                                        "type": "string",
                                        "example": "itor.carlos@example.com"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "201": {
                        "description": "User created successfully",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/User"
                                }
                            }
                        }
                    },
                    "400": {
                        "description": "Validation error"
                    }
                }
            }
        }
    },
    "components": {
        "schemas": {
            "User": {
                "title": "User",
                "required": [
                    "cpf",
                    "name",
                    "data_nascimento"
                ],
                "properties": {
                    "cpf": {
                        "description": "CPF",
                        "type": "string"
                    },
                    "name": {
                        "description": "Name",
                        "type": "string"
                    },
                    "data_nascimento": {
                        "description": "Data de nascimento",
                        "type": "string",
                        "format": "date"
                    }
                },
                "type": "object"
            }
        }
    },
    "tags": [
        {
            "name": "Users",
            "description": "Users"
        }
    ]
}